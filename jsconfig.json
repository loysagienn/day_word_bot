    "baseUrl": "./src",
    "paths": {
      "app/*": [ "app/*", "app/*/index.mjs" ],
      "config": [ "config/production.mjs" ],
      "env/*": [ "server/*" ]
    },
    "module": "commonJS",
    "target": "esnext",
